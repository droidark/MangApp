<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="net.virux.mangapp.model.Profile" table="map_profile">
		<id column="id_profile" name="idProfile" type="integer">
			<generator class="increment"/>
		</id>
		<property name="profile" column="profile" type="string"/>
		
		<!-- RELATIONSHIP MANY-TO-MANY [TABLE: map_user_x_profile]-->
		<set name="users" table="map_user_x_profile" inverse="true" lazy="true" 
				fetch="select">
			<key>
				<column name="id_profile" not-null="true"/>
			</key>
			<many-to-many entity-name="net.virux.mangapp.model.User">
				<column name="id_user" not-null="true"/>
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>